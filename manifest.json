{
  "manifest_version": 2,
  "name": "Password Generator",
  "version": "1.0",
  "description": "Generate password",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "password_generator.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "img/password.png",
    "default_title": "Password Generator",
    "default_popup": "popup/index.html"
  }
}